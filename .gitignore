data/**/*.mp3
data/**/*.wav
data/**/*.pbf
data/**/*.zip
data/**/*.tar
data/**/*.gz
data/**/*.bin
data/**/*.gif
data/telegram/**/media/

# Large data files by type
data/**/*.csv
data/**/*.json
data/**/*.jsonl
data/**/*.geojson
data/**/*.nc
data/**/*.hdf5
data/**/*.npz
data/**/*.npy
data/**/*.tif
